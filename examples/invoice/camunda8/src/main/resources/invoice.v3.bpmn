      "id": "Field_0feflul",
      "key": "invoiceCategory",
      "disabled": false,
      "defaultValue": "Travel Expenses"
    },
    {
      "label": "Invoice Number",
      "type": "textfield",
      "id": "Field_09olxc6",
      "key": "invoiceNumber",
      "disabled": false,
      "defaultValue": "I-12345",
      "description": "(e.g. \"I-12345\")"
    }
  ],
  "type": "default",
  "id": "Form_0jc1o9x",
